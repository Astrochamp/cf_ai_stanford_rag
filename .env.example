# JWT Authentication Keys
# Private key for signing JWTs (PEM format, single line with \n for new lines)
JWT_PRIVATE_KEY=

# Worker's public key for verifying worker authentication (PEM)
WORKER_PUBLIC_KEY=

# Server Configuration
# URL of this Express server (used for JWT audience validation)
EXPRESS_SERVER_URL="http://localhost:3000"

# Port for the Express server (optional, defaults to 3000)
PORT="3000"

# Frontend Configuration
# URL of the frontend application for CORS (defaults to http://localhost:5173)
FRONTEND_URL="http://localhost:5173"

# Cloudflare Configuration
# Your Cloudflare account ID
CLOUDFLARE_ACCOUNT_ID=

# Cloudflare API token with Workers AI read and write permissions
CLOUDFLARE_API_TOKEN=

# URL of the database worker
DB_WORKER_URL=

# OpenAI Configuration
# OpenAI API key for embeddings and completions
OPENAI_API_KEY=

# Cloudflare Turnstile
# Secret key for Turnstile verification
TURNSTILE_SECRET_KEY=
